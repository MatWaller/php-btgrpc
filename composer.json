{
    "name": "matwaller/php-btgrpc",
    "type": "library",
    "description": "Bitcoin Gold JSON-RPC client based on GuzzleHttp",
    "keywords": ["BTG", "Bitcoin Gold", "api","jsonrpc","guzzle"],
    "homepage": "https://github.com/waller/php-btgrpc",
    "license": "MIT",
    "authors": [
        {
            "name": "Mat Waller",
            "email": "Mwaller@devcrypto.co.uk",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=5.6.0",
        "guzzlehttp/guzzle": "^6.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7 || ^6.4 || ^7.0"
    },
    "autoload": {
        "psr-4" : {
            "Waller\\Gold\\": "src"
        },
        "classmap" : [
            "tests"
        ],
        "files" : [
            "src/functions.php"
        ]
    }
}
